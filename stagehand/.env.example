# Google Gemini API key - PREFERRED for browser-based AI tasks
# Gemini is the recommended LLM for Stagehand browser automation
GOOGLE_API_KEY=AIzaSy...

# Run browser in headless mode (set to true for CI/server environments)
HEADLESS=false

# Optional: Anthropic API key (alternative to Gemini)
# ANTHROPIC_API_KEY=sk-ant-...

# Optional: Browserbase for cloud browser infrastructure
# BROWSERBASE_API_KEY=bb_...
# BROWSERBASE_PROJECT_ID=...

# Optional: Enable verbose logging
# STAGEHAND_VERBOSE=true
